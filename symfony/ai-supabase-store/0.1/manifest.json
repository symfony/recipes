{
    "add-lines": [
        {
            "file": "config/packages/ai.yaml",
            "position": "after_target",
            "warn_if_missing": true,
            "target": "    store:",
            "content": "        supabase:\n            default:\n                url: '%env(SUPABASE_URL)%'\n                api_key: '%env(SUPABASE_API_KEY)%'\n                table: 'embeddings'"
        }
    ],
    "env": {
        "#1": "SUPABASE_URL=",
        "#2": "SUPABASE_API_KEY="
    }
}
